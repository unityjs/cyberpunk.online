    "isomorphic-fetch": "^3.0.0",
    "node-fetch": "^3.3.0",
    "socks-proxy-agent": "^7.0.0",
    "vhost": "3.0.2",
    "ws": "^7.1.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "mongodb": "^3.1",
    "node-ip2region": "^1.0.2",
    "jsonwebtoken": "^8.5.1",
    "http-status-codes": "^2.1.4",
